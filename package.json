{
  "name": "simple-logistics-client",
  "version": "0.0.1",
  "description": "Simple logistics web client application",
  "main": "gulpfile.js",
  "scripts": {
    "start": "npm run build:all && concurrently --kill-others \"npm run watch\" \"npm run server\" ",
    "start:app": "npm run build:app && concurrently --kill-others \"npm run watch\" \"npm run server\" ",
    "build:all": "gulp build:all",
    "build:app": "gulp build:app",
    "build:lib": "gulp lib",
    "watch": "gulp watch",
    "server": "lite-server"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/andrei-radzetski/simple-logistics-client.git"
  },
  "keywords": [
    "Angular2",
    "Simple",
    "Logistics"
  ],
  "author": "andrei.radzetski",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/andrei-radzetski/simple-logistics-client/issues"
  },
  "homepage": "https://github.com/andrei-radzetski/simple-logistics-client#readme",
  "devDependencies": {
    "babel-preset-es2015": "^6.18.0",
    "babelify": "^7.3.0",
    "browserify": "^13.1.1",
    "concurrently": "^3.1.0",
    "gulp": "^3.9.1",
    "gulp-concat": "^2.6.0",
    "gulp-if": "^2.0.1",
    "gulp-rename": "^1.2.2",
    "gulp-sass": "^2.3.2",
    "gulp-sourcemaps": "^2.2.0",
    "gulp-uglify": "^2.0.0",
    "gulp-util": "^3.0.7",
    "lite-server": "^2.2.2",
    "tsify": "^2.0.2",
